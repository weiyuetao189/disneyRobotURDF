<mujoco model="liftingMachine">
  <!-- <compiler angle="radian" meshdir="../meshes/"/> -->

  <asset>
    <mesh name="chassisLink" file="liftingMachineMeshes/chassisLink.STL"/>
    <mesh name="cantileverBeamLink" file="liftingMachineMeshes/cantileverBeamLink.STL"/>
    <mesh name="hangerLink" file="liftingMachineMeshes/hangerLink.STL"/>
    <mesh name="electricRod1Link" file="liftingMachineMeshes/electricRod1Link.STL"/>
    <mesh name="electricRod2Link" file="liftingMachineMeshes/electricRod2Link.STL"/>
  </asset>

  <worldbody>
    <body name="worldLink" pos="0 0 0">
      <body name="guideZLink" pos="0 0 0.00">
        <joint name="guideZJoint" type="hinge" axis="0 0 1" range="-31.4 31.4" limited="true" damping="0.01"/>
        <geom type="sphere" size="0.01" pos="0 0 0" rgba="1 0 0 1" density="1000"/>
        <body name="chassisLink" pos="0 0 0.00">
          <joint name="guideXJoint" type="slide" axis="1 0 0" range="-200 200" limited="true" damping="0.01"/>
          <joint name="guideYJoint" type="slide" axis="0 1 0" range="-200 200" limited="true" damping="0.01"/>
          <!-- <freejoint/> -->
          <!-- <inertial pos="-0.85372 2.0777e-08 0.29215" quat="0.987813 0 -0.155645 0" mass="4.667" diaginertia="0.383101 0.35636 0.243489"/> -->
          <geom type="mesh" contype="0" conaffinity="0" group="1" density="1000" rgba="1 1 1 1" mesh="chassisLink"/>
          <!-- <geom type="mesh" rgba="1 1 1 1" mesh="chassisLink"/> -->
          <body name="cantileverBeamLink" pos="-1.2007 0 1.5368">
            <!-- <inertial pos="0.62321 2.2053e-09 -0.027975" quat="0.475289 0.523546 0.523546 0.475289" mass="0.91394" diaginertia="0.11142 0.110087 0.00172023"/> -->
            <joint name="cantileverBeamJoint" pos="0 0 0" axis="0 1 0" range="-1.3 0.645" damping="0.1"/>
            <geom type="mesh" contype="0" conaffinity="0" group="1" density="1000" rgba="1 1 1 1" mesh="cantileverBeamLink"/>
            <!-- <geom type="mesh" rgba="1 1 1 1" mesh="cantileverBeamLink"/> -->
            <body name="hangerLink" pos="1.1957 0 -0.22395">
              <!-- <inertial pos="-6.1228e-10 -7.5765e-11 -0.09887" quat="0.707107 0.707107 0 0" mass="0.24758" diaginertia="0.0024844 0.0023255 0.00016362"/> -->
              <!-- <joint name="hangerJoint" pos="0 0 0" type="ball" damping="0.001"/> -->
              <joint name="hangerYJoint" pos="0 0 0" axis="0 1 0" range="-1 1" damping="0.1"/>
              <!-- <joint name="hangerZJoint" pos="0 0 0" axis="0 0 1" range="-3.14 3.14" damping="0.1"/> -->
              <geom type="mesh" contype="0" conaffinity="0" group="1" density="1000" rgba="1 1 1 1" mesh="hangerLink"/>
              <!-- <geom type="mesh" rgba="1 1 1 1" mesh="hangerLink"/> -->
              <site name="hangingPoint1" pos="0 0.180 -0.115" rgba="0 .8 0 1"/>     
              <site name="hangingPoint2" pos="0 -0.180 -0.115" rgba="0 .8 0 1"/>     
            </body>
          </body>
          <body name="electricRod1Link" pos="-1.0125 0 0.75763">
            <!-- <inertial pos="0.041602 0 0.27309" quat="0.997145 0 0.0755146 0" mass="1.0758" diaginertia="0.0223902 0.02239 0.000598263"/> -->
            <joint name="electricRod1Joint" pos="0 0 0" axis="0 1 0" range="-0.2 0.2"/>
            <geom type="mesh" contype="0" conaffinity="0" group="1" density="1000" rgba="1 1 1 1" mesh="electricRod1Link"/>
            <!-- <geom type="mesh" rgba="1 1 1 1" mesh="electricRod1Link"/> -->
            <body name="electricRod2Link">
              <!-- <inertial pos="0.071323 0 0.46819" quat="0.705088 0.0534003 0.0534003 0.705088" mass="0.3689" diaginertia="0.0073856 0.00738527 4.15758e-05"/> -->
              <joint name="electricRod2Joint" pos="0 0 0" axis="0.150601 0 0.988595" type="slide" range="-0.2 1" limited="true" damping="0.01"/>
              <geom type="mesh" contype="0" conaffinity="0" group="1" density="1000" rgba="1 1 1 1" mesh="electricRod2Link"/>
              <!-- <geom type="mesh" rgba="1 1 1 1" mesh="electricRod2Link"/> -->
            </body>
          </body>
        </body>
      </body>
    </body>
  </worldbody>

  <actuator>
    <position joint="cantileverBeamJoint" name="cantileverBeamJointMotor" kp="5000" kv="50" ctrlrange="-1.3 0.645"/>
    <position joint="guideXJoint" name="guideXJointMotor" ctrlrange="-200 200"/>
    <position joint="guideYJoint" name="guideYJointMotor" ctrlrange="-200 200"/>
    <position joint="guideZJoint" name="guideZJointMotor" ctrlrange="-200 200"/>
    <!-- <motor gear="1" joint="cantileverBeamJoint" name="cantileverBeamJointMotor" ctrllimited="true" ctrlrange="-160 0" />  kp="100" kv="1" -->
  </actuator>

  <equality>
    <connect body1="cantileverBeamLink" body2="electricRod2Link" anchor="0.300 0 -0.045"/>
  </equality>
</mujoco>